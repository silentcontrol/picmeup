    "ngrok": "^3.1.0",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-dom-camera": "^0.3.2",
    "react-html5-camera-photo": "^1.2.8-beta.0",
    "react-modal": "^3.6.1",
